{
	"name": "omnidev-capabilities",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"description": "Community capabilities for OmniDev",
	"scripts": {
		"typecheck": "tsc --noEmit",
		"lint:check": "biome lint .",
		"lint": "biome lint --write .",
		"format": "biome format --write .",
		"format:check": "biome format .",
		"check": "npm run typecheck && npm run lint:check && npm run format:check",
		"test": "node --test --experimental-test-coverage --experimental-strip-types **/*.test.ts"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@types/node": "^22.0.0",
		"typescript": "^5.7.2",
		"lefthook": "^2.0.13"
	},
	"dependencies": {
		"@omnidev-ai/core": "latest"
	}
}
